{
  "name": "@cspark/sdk",
  "version": "0.3.1-rc.1",
  "description": "A JavaScript SDK for interacting with Coherent Spark APIs",
  "author": "Coherent <team@coherent.global>",
  "homepage": "https://github.com/Coherent-Partners/spark-ts-sdk/blob/main/docs/readme.md",
  "license": "Apache-2.0",
  "keywords": [
    "coherent",
    "spark",
    "sdk",
    "restful",
    "api"
  ],
  "repository": "github:Coherent-Partners/spark-ts-sdk",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.mjs",
  "types": "./lib/types.d.ts",
  "browser": "./lib/bundle.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^14.15.0 || >=16.0.0"
  },
  "scripts": {
    "start": "echo '>>> no start command specified' && exit 0",
    "format": "prettier --write '{src,test,examples}/**/*.(t|j)s'",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "test": "jest",
    "prebuild": "shx rm -rf lib",
    "build": "tsc && rollup -c",
    "demo": "tsx examples/index.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": "eslint '{src,test}/**/*.ts' --cache --fix",
    "*.{md,ts,json,yml}": "prettier -u --write '{src,test,examples,docs}/**/*'"
  },
  "dependencies": {
    "buffer": "6.0.3",
    "form-data": "4.0.0",
    "node-fetch": "2.7.0",
    "pako": "~2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "29.5.11",
    "@types/node": "*",
    "@types/node-fetch": "^2.6.11",
    "@types/pako": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "abort-controller": "^3.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "jwt-decode": "^4.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup": "^4.4.0",
    "rollup-plugin-dts": "^6.1.1",
    "shx": "^0.4.0",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsx": "^4.20.1",
    "typescript": "~5.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "ecosystem/",
      "packages/"
    ],
    "coverageDirectory": "coverage",
    "clearMocks": true,
    "transform": {
      "^.+.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.dev.json"
        }
      ]
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/streaming.ts",
      "!src/logger.ts",
      "!src/utils.ts"
    ]
  }
}
