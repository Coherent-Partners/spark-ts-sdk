{
  "name": "@cspark/sdk",
  "version": "0.1.1",
  "description": "A Node.js SDK for interacting with Coherent Spark APIs",
  "author": "Ralph Florent",
  "homepage": "https://github.com/c-spark/cspark-ts-sdk/blob/main/docs/readme.md",
  "license": "Apache-2.0",
  "keywords": [
    "coherent",
    "spark",
    "sdk",
    "restful",
    "api"
  ],
  "repository": "github:c-spark/cspark-ts-sdk",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "browser": "./lib/bundle.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^14.15.0 || >=16.0.0"
  },
  "scripts": {
    "start": "echo \">>> no start command specified\" && exit 0",
    "format": "prettier --write \"{src,test,examples}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "prebuild": "rm -rf lib",
    "build": "tsc && rollup -c",
    "postbuild": "echo '{\"type\":\"module\"}' > lib/esm/package.json",
    "demo": "tsc -p tsconfig.dev.json && node lib/examples/index.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": "eslint \"{src,test}/**/*.ts\" --cache --fix",
    "*.{md,ts,json,yml}": "prettier -u --write \"{src,test,examples,docs}/**/*\""
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.1",
    "@types/node": "*",
    "@types/node-fetch": "^2.6.11",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup": "^4.4.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "*"
  },
  "exports": {
    ".": {
      "types": "./lib/esm/index.d.ts",
      "require": "./lib/cjs/index.js"
    },
    "./*": {
      "types": "./lib/esm/*.d.ts",
      "require": "./lib/cjs/*.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "jest": {
    "rootDir": "test",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "coverageDirectory": "../coverage",
    "clearMocks": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.dev.json"
        }
      ]
    },
    "moduleNameMapper": {
      "^@cspark/sdk$": "<rootDir>/../src/index.ts",
      "^@cspark/sdk/(.*)$": "<rootDir>/../src/$1"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ]
  }
}
